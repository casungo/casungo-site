{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@js/*": [
        "src/js/*"
      ],
      "@css/*": [
        "src/styles/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  }
}